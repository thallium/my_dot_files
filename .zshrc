FLAGS_warning="-Wall -Wextra -Wconversion -Wshift-overflow=2 "
FLAGS_SANI="-fsanitize=address -fsanitize=undefined -fno-sanitize-recover=all "
comp(){
    FLAG="-std=c++1z -O2 ${FLAGS_warning}${FLAGS_SANI}"
    while getopts ":fo" opt; do
        case $opt in
            f)
                FLAG="-std=c++1z -O2 ${FLAGS_warning}"
                ;;
            o)
                FLAG="-std=c++11 -O2 ${FLAGS_warning}${FLAGS_SANI}"
                ;;
            \?) 
                echo "Invalid option: -$OPTARG" >&2 ;;
        esac
    done
    eval g++ $FLAG "${@: -1}".cpp -o "${@: -1}"

}

ex () {
  if [ -f $1 ] ; then
    case $1 in
      *.tar.bz2)   tar xjf $1   ;;
      *.tar.gz)    tar xzf $1   ;;
      *.bz2)       bunzip2 $1   ;;
      *.rar)       unrar x $1     ;;
      *.gz)        gunzip $1    ;;
      *.tar)       tar xf $1    ;;
      *.tbz2)      tar xjf $1   ;;
      *.tgz)       tar xzf $1   ;;
      *.zip)       unzip $1     ;;
      *.Z)         uncompress $1;;
      *.7z)        7z x $1      ;;
      *)           echo "'$1' cannot be extracted via ex()" ;;
    esac
  else
    echo "'$1' is not a valid file"
  fi
}

